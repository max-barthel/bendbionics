{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:pre-commit": "vitest --run --coverage --reporter=verbose",
    "test:ci": "vitest --run --coverage --reporter=junit --outputFile=test-results.xml",
    "test:integration": "playwright test",
    "test:integration:ui": "playwright test --ui",
    "test:integration:debug": "playwright test --debug",
    "test:visual": "playwright test visual-regression.spec.ts",
    "test:all": "npm run test:run && npm run test:integration",
    "test:install": "playwright install",
    "build:analyze": "ANALYZE=true npm run build",
    "build:size-check": "npm run build && node scripts/bundle-size-check.js",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --upload.target=filesystem",
    "performance": "npm run build:size-check && npm run lighthouse",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "storybook build && npx serve storybook-static",
    "docs": "npm run storybook",
    "docs:build": "npm run storybook:build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tauri-apps/api": "^1.5.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.11.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.8.0",
    "three": "^0.179.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.30.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.1.7",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-backgrounds": "^9.0.8",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-measure": "^9.0.8",
    "@storybook/addon-onboarding": "^9.1.7",
    "@storybook/addon-outline": "^9.0.8",
    "@storybook/addon-toolbars": "^9.0.8",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/addon-vitest": "^9.1.7",
    "@storybook/react-vite": "^9.1.7",
    "@tailwindcss/postcss": "^4.1.11",
    "@tauri-apps/cli": "^1.5.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.9.36",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/three": "^0.178.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-storybook": "^9.1.7",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "markdown-to-jsx": "^7.7.13",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^9.1.7",
    "tailwindcss": "^4.1.11",
    "tailwindcss-cli": "^0.1.2",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
